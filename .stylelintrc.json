{
  "extends": "stylelint-config-standard",
  "rules": {
    "indentation": 2,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "mixin",
          "extend",
          "return",
          "function",
          "content",
          "include",
          "lost-column",
          "if"
        ]
      }
    ]
  }
}
